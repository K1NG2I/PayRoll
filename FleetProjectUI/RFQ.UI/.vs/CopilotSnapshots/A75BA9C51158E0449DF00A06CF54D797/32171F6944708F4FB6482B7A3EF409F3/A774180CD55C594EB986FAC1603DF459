    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script>

        $(document).ready(function () {
           var profileid = getCookieValue("profileid");
           var UserEmail = getCookieValue("UserEmail");
           var PersonName = getCookieValue("PersonName");

           if(profileid != 1)
              $("#resetPassword").addClass('d-none');
           else
                $("#resetPassword").removeClass('d-none');

            $("#UserEmail").text(decodeURIComponent(UserEmail));
            $("#PersonName").text(PersonName);

            showInitial(PersonName);

            // Handle menu link clicks. For menu items (submenu links) we want to
            // inspect the LinkUrl and navigate to the corresponding controller/action.
            // Skip links that are used for collapsing menus (have data-bs-toggle) or
            // anchors/javascript.
            $('#leftside-menu-container').on('click', 'a.side-nav-link', function (e) {
                var $a = $(this);
                var href = $a.attr('href');

                // If this is a collapse toggle or has no href or is an anchor/javascript, do nothing
                if ($a.attr('data-bs-toggle') || !href || href.startsWith('javascript') || href === '#') {
                    return;
                }

                // Prevent the default and handle navigation
                e.preventDefault();

                // Absolute or root-relative URL -> navigate directly
                if (href.startsWith('/') || href.startsWith('http')) {
                    window.location.href = href;
                    return;
                }

                // Otherwise treat href as controller/action or controller/action?qs
                var parts = href.split('?');
                var path = parts[0];
                var qs = parts.length > 1 ? '?' + parts.slice(1).join('?') : '';

                // Ensure leading slash and navigate
                if (!path.startsWith('/')) path = '/' + path + '/index';
                window.location.href = path + qs;
            });
        });

        function toggleDropdown(element) {
            element.classList.toggle('dropdown-open');
        }

        function showInitial(userName) {
                 const firstChar = userName.charAt(0).toUpperCase();
                 const avatarImage = document.getElementById("avatarImage");

                 const fixedColor = "#1d68a7"; // Deep blue
                 avatarImage.innerText = firstChar;
                 avatarImage.style.backgroundColor = fixedColor;
                 avatarImage.style.width = "50px";
                 avatarImage.style.height = "50px";
                 avatarImage.style.borderRadius = "50%";
                 avatarImage.style.color = "#ffffff"; // white font
                 avatarImage.style.fontSize = "24px";
                 avatarImage.style.fontWeight = "bold";
                 avatarImage.style.fontFamily = "Arial, sans-serif";
                 avatarImage.style.display = "flex";
                 avatarImage.style.alignItems = "center";
                 avatarImage.style.justifyContent = "center";
                 avatarImage.style.userSelect = "none";
        }
    </script>

