                        <!--- Sidemenu -->
                        <ul id="leftside-menu-container" class="side-nav">
                            @if (menuList != null && menuList.Any())
                            {
                                @* var groupedMenus = menuList
                                .OrderBy(x => x.SequenceNo)
                                .GroupBy(x => new { x.LinkGroupId, x.LinkGroupName, x.LinkGroupIcon }); *@
                                var groupedMenus = menuList.OrderBy(x => x.SequenceNo)
                                .GroupBy(x => new { x.LinkGroupId, x.LinkGroupName, x.LinkGroupIcon,x.GroupSequenceNo })
                                .OrderBy(g => g.Min(m => m.GroupSequenceNo)); 

                                foreach (var group in groupedMenus)
                                {
                                    var collapseId = "submenu-" + group.Key.LinkGroupId;

                                    <li class="side-nav-item dropdown">
                                        <a class="side-nav-link"
                                           data-bs-toggle="collapse"
                                           data-bs-target="@collapseId"
                                           aria-expanded="false"
                                           aria-controls="@collapseId">
                                            <i class="@group.Key.LinkGroupIcon"></i>
                                            <span>@group.Key.LinkGroupName</span>
                                        </a>
                                        <ul class="sidebar-submenu collapse" id="@collapseId" data-bs-parent="@collapseId">
                                            @foreach (var item in group.OrderBy(x => x.SequenceNo))
                                            {
                                                <li>
                                                    <a href="@item.LinkUrl" class="side-nav-link">
                                                        <i class="@item.LinkIcon"></i>
                                                        <span>@item.LinkName</span>
                                                    </a>
                                                </li>
                                            }
                                        </ul>
                                    </li>
                                }
                            }
                            else
                            {
                                <p>NO Menu</p>
                            }
                        </ul>
                        <!--- End Sidemenu -->

                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
