@{
    Layout = "_Layout";
    ViewData["Title"] = "_Sidebar";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <link rel="shortcut icon" href="~/favicon.ico" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Inter:300,400,500,600,700" />
    <link href="~/css/plugins.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/all.min.css" rel="stylesheet" type="text/css" />
    <link rel="~/stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link href="~/css/common.css" rel="stylesheet" type="text/css" />
    <link href="~/css/style.css" rel="stylesheet" type="text/css" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>


    <script src="~/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/app.min.js"></script>
    <script src="~/js/sidebarmenu.js"></script>
    @* <script src="~/js/dashboard.js"></script> *@
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    @* <script src="~/js/plugins.bundle.js"></script> *@
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/script.js"></script>
</head>
<body>

    <!-- Sidebar Start -->
    <aside class="app-sidebar-menu overflow-hidden">
        <div class="app-sidebar-wrapper my-5">
            <div class="hover-scroll-overlay-y my-5 mx-4">
                <div class="menu menu-column menu-rounded menu-sub-indention fw-semibold px-1" id="sidebarnav">
                    <div class="menu-item">
                        <a class="menu-link" href="@Url.Action("Dashboard", "Home")">
                            <span class="menu-icon"><i class="ti ti-layout-dashboard fs-1"></i></span>
                            <span class="menu-title">Dashboard</span>
                        </a>
                    </div>
                    <div class="menu-item menu-accordion" id="dynamic-menu"></div>
                    <div class="menu-item">
                        <a class="menu-link" href="@Url.Action("ResetPassword", "Home")">
                            <span class="menu-icon"><i class="ti ti-key fs-1"></i></span>
                            <span class="menu-title">Reset Password</span>
                        </a>
                    </div>
                    <div class="menu-item">
                        <a class="menu-link" href="@Url.Action("ChangePassword", "Home")">
                            <span class="menu-icon"><i class="ti ti-lock fs-1"></i></span>
                            <span class="menu-title">Change Password</span>
                        </a>
                    </div>
                </div>
            </div>
    </aside>
    <!-- Sidebar End -->

</body>



</html>
